<h1><%= @tape.name %></h1>

<div id="user_container">

  <div id="tape_container">
    <!-- <button id="search">Search For Songs</button><br/> -->
    <ul>
      <li><img id="<%= @tape.id %>" src="<%= @tape.img_url %>"width="350px"/></li><br/>

      <!-- <i class="fa fa-play fa-5x"></i> -->
      <div class="button button-circle button-action">PLAY</div>
     <!-- <button id="pause">PAUSE</button> -->
      <!-- <div class="small-button small-button-circle button-action">PAUSE</div> -->

    </ul>
  </div>
  <div id="tracks">
    <ul>
      <li><%= @tape.name %></li>
      <% @songs.each do |song| %>
       <li><%= song.name %></li>
      <% end %>
    </ul>
  </div>


</div>

